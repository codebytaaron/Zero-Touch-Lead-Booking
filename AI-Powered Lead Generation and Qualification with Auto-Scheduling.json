    {
      "parameters": {
        "schemaType": "manual",
        "inputSchema": "{\n\t\"type\": \"object\",\n\t\"properties\": {\n\t\t\"score\": {\n\t\t\t\"type\": \"number\",\n\t\t\t\"description\": \"Lead quality score from 0-100\"\n\t\t},\n\t\t\"reasoning\": {\n\t\t\t\"type\": \"string\",\n\t\t\t\"description\": \"Brief explanation of the score\"\n\t\t},\n\t\t\"priority\": {\n\t\t\t\"type\": \"string\",\n\t\t\t\"enum\": [\"high\", \"medium\", \"low\"],\n\t\t\t\"description\": \"Priority level for follow-up\"\n\t\t},\n\t\t\"recommendedAction\": {\n\t\t\t\"type\": \"string\",\n\t\t\t\"description\": \"Suggested next steps\"\n\t\t}\n\t},\n\t\"required\": [\"score\", \"reasoning\", \"priority\", \"recommendedAction\"]\n}"
      },
      "id": "605cc27d-a000-4d76-97c3-277d94ea1f64",
      "name": "Lead Qualification Schema",
      "type": "@n8n/n8n-nodes-langchain.outputParserStructured",
      "typeVersion": 1.3,
      "position": [
        2120,
        128
      ]
    },
    {
      "parameters": {
        "promptType": "define",
        "text": "=Analyze this lead:\n\nName: {{ $json.fullName }}\nEmail: {{ $json.email }}\nCompany: {{ $json.company }}\nCompany Size: {{ $json.companySize }}\nIndustry: {{ $json.industry }}\nChallenges: {{ $json.challenges }}\nTimeline: {{ $json.timeline }}\nBudget: {{ $json.budget }}",
        "hasOutputParser": true,
        "options": {
          "systemMessage": "You are a lead qualification expert. Analyze lead information and provide a quality score (0-100) based on these criteria:\n\n1. Company Size (weight: 20%) - Larger companies typically have bigger budgets\n2. Budget Range (weight: 30%) - Higher budgets indicate stronger intent\n3. Timeline (weight: 25%) - Immediate/short-term needs are higher priority\n4. Challenge Complexity (weight: 15%) - Detailed, specific challenges show serious intent\n5. Industry Fit (weight: 10%) - Technology, Finance, Healthcare are typically good fits\n\nProvide a score, reasoning, priority level (high/medium/low), and recommended action. Leads scoring 70+ should be marked as high priority."
        }
      },
      "id": "8ac59a40-9956-4cfe-9110-3f7ca66602b4",
      "name": "AI Lead Qualifier",
      "type": "@n8n/n8n-nodes-langchain.agent",
      "typeVersion": 3.1,
      "position": [
        1984,
        -96
      ]
    },
    {
      "parameters": {
        "conditions": {
          "options": {
            "caseSensitive": false,
            "leftValue": "",
            "typeValidation": "loose"
          },
          "conditions": [
            {
              "id": "id-1",
              "leftValue": "={{ $json.score }}",
              "rightValue": "70",
              "operator": {
                "type": "number",
                "operation": "gte"
              }
            }
          ],
          "combinator": "and"
        },
        "options": {}
      },
      "id": "7fe6f126-ef41-4cfc-94e5-3a440f038301",
      "name": "Route High-Score Leads",
